#!/bin/bash

count=$1; shift
WORKER_ARGS=$@

mkdir -p ${USER}-workers >/dev/null 2>&1
pushd ${USER}-workers >/dev/null 2>&1
cat >worker.sh <<EOF
#!/bin/bash
./work_queue_worker $@
EOF
chmod 755 worker.sh
cp `which work_queue_worker` .

qsub -pe smp 12 -q long -t 1-$count:1 -cwd  worker.sh --  $WORKER_ARGS
